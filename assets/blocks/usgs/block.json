{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "kwcusgs/usgs",
	"version": "1.0.0",
	"title": "usgs",
	"category": "text",
	"icon": "smiley",
	"description": "Example static block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "example-static",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  "attributes": {
		"message": {
			"type": "string",
			"default": "Hello from a dynamic block!"
		},
    "size": {
			"type": "string",
			"default": "small"
		}
	},
	"example": {
		"attributes": {
			"message": "Example Dynamic Block"
		}
	}
}
